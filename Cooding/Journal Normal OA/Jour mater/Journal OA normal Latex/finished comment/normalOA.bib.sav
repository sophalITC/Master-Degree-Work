@Article{Yuan2013,
  author   = {Yuan, Jing and Qiu, Wu and Rajchl, Martin and Ukwatta, Eranga and Tai, Xue Cheng and Fenster, Aaron},
  title    = {{Efficient 3D endfiring TRUS prostate segmentation with globally optimized rotational symmetry}},
  journal  = {Proceedings of the IEEE Computer Society Conference on Computer Vision and Pattern Recognition},
  year     = {2013},
  pages    = {2211--2218},
  issn     = {10636919},
  abstract = {Segmenting 3D end firing transrectal ultrasound (TRUS) prostate images $\backslash$nefficiently and accurately is of utmost importance for the planning and guiding $\backslash$n3D TRUS guided prostate biopsy. Poor image quality and imaging artifacts of 3D $\backslash$nTRUS images often introduce a challenging task in computation to directly $\backslash$nextract the 3D prostate surface. In this work, we propose a novel global $\backslash$noptimization approach to delineate 3D prostate boundaries using its rotational $\backslash$nresliced images around a specified axis, which properly enforces the inherent $\backslash$nrotational symmetry of prostate shapes to jointly adjust a series of 2D slice $\backslash$nwise segmentations in the global 3D sense. We show that the introduced $\backslash$nchallenging combinatorial optimization problem can be solved globally and $\backslash$nexactly by means of convex relaxation. In this regard, we propose a novel $\backslash$ncoupled continuous max-flow model, which not only provides a powerful $\backslash$nmathematical tool to analyze the proposed optimization problem but also amounts $\backslash$nto a new and efficient duality-based algorithm. Extensive experiments $\backslash$ndemonstrate that the proposed method significantly outperforms the state-of-art $\backslash$nmethods in terms of efficiency, accuracy, reliability and less $\backslash$nuser-interactions, and reduces the execution time by a factor of 100.},
  doi      = {10.1109/CVPR.2013.287},
  file     = {:C$\backslash$:/Users/ASUS/Downloads/Texture analysis reference/prostate segmentation1.pdf:pdf},
  isbn     = {978-0-7695-4989-7},
  keywords = {3D TRUS,Continuous Max-Flow,Convex Optimization,Image Segmentation},
}

@Article{Esgiar2002,
  author   = {Esgiar, Abdelrahim Nasser and Naguib, Raouf N G and Sharif, Bayan S and Bennett, Mark K and Murray, Alan},
  title    = {{Fractal analysis in the detection of colonic cancer images.}},
  journal  = {IEEE transactions on information technology in biomedicine : a publication of the IEEE Engineering in Medicine and Biology Society},
  year     = {2002},
  volume   = {6},
  number   = {1},
  pages    = {54--8},
  issn     = {1089-7771},
  abstract = {The aim of this study was to investigate the value of fractal dimension in separating normal and cancerous images, and to examine the relationship between fractal dimension and traditional texture analysis features. Forty-four normal images and 58 cancer images from sections of the colon were analyzed. A "leave-one-out" analysis approach was used to classify the samples into each group. With fractal analysis there was a highly significant difference between groups (p {\textless} 0.0001). Correlation and entropy features showed greater differences between the groups (p {\textless} 0.0001). Nevertheless, the addition of fractal analysis to the feature analysis improved the sensitivity from 90{\%} to 95{\%} and specificity from 86{\%} to 93{\%}.},
  doi      = {10.1109/4233.992163},
  file     = {:C$\backslash$:/Users/ASUS/Downloads/Texture analysis reference/colon cancer.pdf:pdf},
  keywords = {Case-Control Studies,Colonic Neoplasms,Colonic Neoplasms: pathology,Fractals,Humans},
  pmid     = {11936597},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/11936597},
}

@Article{ValdesHernandez2017,
  author   = {{Vald{\'{e}}s Hern{\'{a}}ndez}, Maria del C. and Gonz{\'{a}}lez-Castro, Victor and Chappell, Francesca M. and Sakka, Eleni and Makin, Stephen and Armitage, Paul A. and Nailon, William H. and Wardlaw, Joanna M.},
  title    = {{Application of Texture Analysis to Study Small Vessel Disease and Blood–Brain Barrier Integrity}},
  journal  = {Frontiers in Neurology},
  year     = {2017},
  volume   = {8},
  number   = {July},
  issn     = {1664-2295},
  abstract = {OBJECTIVES We evaluate the alternative use of texture analysis for evaluating the role of blood-brain barrier (BBB) in small vessel disease (SVD). METHODS We used brain magnetic resonance imaging from 204 stroke patients, acquired before and 20 min after intravenous gadolinium administration. We segmented tissues, white matter hyperintensities (WMH) and applied validated visual scores. We measured textural features in all tissues pre- and post-contrast and used ANCOVA to evaluate the effect of SVD indicators on the pre-/post-contrast change, Kruskal-Wallis for significance between patient groups and linear mixed models for pre-/post-contrast variations in cerebrospinal fluid (CSF) with Fazekas scores. RESULTS Textural "homogeneity" increase in normal tissues with higher presence of SVD indicators was consistently more overt than in abnormal tissues. Textural "homogeneity" increased with age, basal ganglia perivascular spaces scores (p {\textless} 0.01) and SVD scores (p {\textless} 0.05) and was significantly higher in hypertensive patients (p {\textless} 0.002) and lacunar stroke (p = 0.04). Hypertension (74{\%} patients), WMH load (median = 1.5 ± 1.6{\%} of intracranial volume), and age (mean = 65.6 years, SD = 11.3) predicted the pre/post-contrast change in normal white matter, WMH, and index stroke lesion. CSF signal increased with increasing SVD post-contrast. CONCLUSION A consistent general pattern of increasing textural "homogeneity" with increasing SVD and post-contrast change in CSF with increasing WMH suggest that texture analysis may be useful for the study of BBB integrity.},
  doi      = {10.3389/fneur.2017.00327},
  file     = {:C$\backslash$:/Users/ASUS/Downloads/Texture analysis reference/Vessel Disease.pdf:pdf},
  isbn     = {1664-2295 (Print)
1664-2295 (Linking)},
  keywords = {age,blood,brain barrier,cerebral small vessel disease,contribution to this work,equal,hypertension,joined first authorship for,leukoaraiosis,perivascular spaces,stroke,texture analysis,texture analysis, cerebral small vessel disease, blood–brain barrier, hypertension, age, stroke, leukoaraiosis, perivascular spaces},
  pmid     = {28769863},
  url      = {http://journal.frontiersin.org/article/10.3389/fneur.2017.00327/full},
}

@Article{Ahmed2013,
  author   = {Ahmed, Arfan and Gibbs, Peter and Pickles, Martin and Turnbull, Lindsay},
  title    = {{Texture analysis in assessment and prediction of chemotherapy response in breast cancer}},
  journal  = {Journal of Magnetic Resonance Imaging},
  year     = {2013},
  volume   = {38},
  number   = {1},
  pages    = {89--101},
  issn     = {10531807},
  abstract = {PURPOSE: To assess the efficacy of dynamic contrast-enhanced magnetic resonance imaging (DCE-MRI)-based textural analysis in predicting response to chemotherapy in a cohort of breast cancer patients. MATERIALS AND METHODS: In all, 100 patients were scanned on a 3.0T HDx scanner immediately prior to neoadjuvant chemotherapy treatment. A software application to use texture features based on co-occurrence matrices was developed. Texture analysis was performed on precontrast and 1-5 minutes postcontrast data. Patients were categorized according to their chemotherapeutic response: partial responders corresponding to a decrease in tumor diameter over 50{\%} (40) and nonresponders corresponding to a decrease of less than 50{\%} (4). Data were also split based on factors that influence response: triple receptor negative phenotype (TNBC) (22) vs. non-TNBC (49); node negative (45) vs. node positive (46); and biopsy grade 1 or 2 (38) vs. biopsy grade 3 (55). RESULTS: Parameters f2 (contrast), f4 (variance), f10 (difference in variance), f6 (sum average), f7 (sum variance), f8 (sum entropy), f15 (cluster shade), and f16 (cluster prominence) showed significant differences between responders and partial responders of chemotherapy. Differences were mainly seen at 1-3 minutes postcontrast administration. No significant differences were found precontrast administration. Node +ve, high grade, and TNBC are associated with poorer prognosis and appear to be more heterogeneous in appearance according to texture analysis. CONCLUSION: This work highlights that textural differences between groups (based on response, nodal status, and triple negative groupings) are apparent and appear to be most evident 1-3 minutes postcontrast administration. The fact that significant differences for certain texture parameters and groupings are consistently observed is encouraging.},
  doi      = {10.1002/jmri.23971},
  file     = {:C$\backslash$:/Users/ASUS/Downloads/Texture analysis reference/breast cancer1.pdf:pdf},
  isbn     = {1522-2586 (Electronic) 1053-1807 (Linking)},
  keywords = {Haralick co-occurrence matrices,MRI contrast enhancement,breast cancer chemotherapy prediction,computer science informatics,image processing,texture analysis software},
  pmid     = {23238914},
}

