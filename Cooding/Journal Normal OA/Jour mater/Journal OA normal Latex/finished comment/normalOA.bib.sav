journal = {Annals of Internal Medicine},
number = {8},
pages = {637--639},
pmid = {11033593},
title = {{NIH Conference Osteoarthritis : New Insights AND I TS}},
url = {http://www.annals.org/content/133/8/635.short},
volume = {133},
year = {2000}
