isbn = {00099260 (ISSN)},
issn = {00099260},
journal = {Clinical Radiology},
keywords = {Computed tomography (CT), tissue characterization,Diagnostic radiology,Images, analysis,Images, processing,Magnetic resonance (MR), tissue characterization,Ultrasound (US), tissue characterization},
number = {12},
pages = {1061--1069},
pmid = {15556588},
title = {{Texture analysis of medical images}},
volume = {59},
year = {2004}
}
@article{Chuah2011,
abstract = {This paper presents a study that investigated the potential of texture analysis using Fluid Sensitive Fat Suppressed MRI images for the use in detection of bone marrow edema. A total of 168 slices of knee MRI from 10 subjects were involved. Six histogram-based textures (mean intensity, standard deviation, smoothness, third moment, uniformity and entropy) were calculated in both 2D and 3D, and were compared between healthy group and group affected by bone marrow edema. Two-sample t-tests were performed to assess the difference between healthy group and group affected by edema. The intensity third moment in 2D showed significant difference between the slices of healthy subjects and the slices affected by edema (p{\textless}0.05). Smoothness and standard deviation in 2D showed a modest significance between healthy and affected groups. No significant difference was found in the 3D textures of healthy group and group affected by edema.},
author = {Chuah, Tong Kuan and Poh, Chueh Loo and Sheah, Kenneth},
doi = {10.1109/IEMBS.2011.6091266},
file = {:C$\backslash$:/Users/ASUS/Documents/MATLAB/Journal Normal OA/Jour mater/Related work/d3.pdf:pdf},
isbn = {9781424441211},
issn = {1557170X},
journal = {Proceedings of the Annual International Conference of the IEEE Engineering in Medicine and Biology Society, EMBS},
keywords = {Image classification,MRI pulse sequence,Multivariate Image Analysis},
pages = {5112--5115},
pmid = {22255489},
title = {{Quantitative texture analysis of MRI images for detection of cartilage-related bone marrow edema}},
year = {2011}
}
@article{Janvier2015,
abstract = {{\textcopyright} 2015 IEEE.This paper presents a preliminary study of the influence of the positioning of Regions Of Interest (ROI) for the characterization of bone texture on radiographs for the diagnosis of knee OsteoArthritis (OA) progression. Characterization of the bone texture is of great interest to doctors because it would improve the prognostic in the clinical routine. In general, studies mainly focus on the descriptors while neglecting the choice of ROI positioning. Using fractal descriptors, the objective of this work is to highlight the impact of the ROI for the diagnosis of knee OA by considering the couple (descriptor, ROI). This study was performed over 1054 knees from 616 subjects composed of stable and progressor patients. Achieved statistical tests demonstrated the importance of the choice of the ROI to improve the clinical diagnosis.},
